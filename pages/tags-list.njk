---
permalink: /tags/
layout: base
---

{% set css %} {% include "css/posts.css" %} {% endset %}
<style>
	{{ css | cssmin | safe }}
</style>

<h1>Tags</h1>

{% for tag in collections.tagList %}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
  <a href="{{ tagUrl | url }}" class="post-tag">{{ tag }}</a>
{% endfor %}
