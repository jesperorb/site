---
layout: base
permalink: /resume/
eleventyNavigation:
  key: Resume
  order: 1
---

{% set css %} {% include "css/resume.css" %} {% endset %}
<style>
	{{ css | cssmin | safe }}
</style>

<div class="timeline">
  {%- for position in resume -%}
  <div class="timeline__date">{{ position.date | shortMonth }}</div>
  <div class="timeline__separator"></div>
  <div class="timeline__content">
    <h2 class="timeline__title">{{ position.title }}</h2>
    <p class="timeline__description">{{ position.description }}</p>
  </div>
  {%- endfor -%}
</div>
